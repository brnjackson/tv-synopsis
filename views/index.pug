html
    head
        link(rel="stylesheet" href="/css/styles.css")
    body
    h1= showdata.title
    h2 Developed By
    ul
      for dev in showdata.developedBy
        li= dev
    h2 Starring
    ul
      for actors in showdata.starring
        li= actors
    h2 Seasons
    ul
      for val in showdata.seasons
        li
          a(href="/seasons/" + val.number) Season #{val.number}
    
 
